<div class="panel panel-default">
  <div class="panel-heading" role="tab">
    <h4 class="panel-title">
      <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-documents" aria-expanded="true" aria-controls="collapseOne">
        Documentos
      </a>
    </h4>
  </div>
  <div id="collapse-documents" class="panel-collapse collapse" role="tabpanel" aria-spanledby="headingOne">
    <div class="panel-body">
      <div class="row panel-body">
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
          <div class="row form-group">
            <%= documents_form.label :cpf, 'CPF:', class: "col-sm-3 control-label" %>
            <div class="col-sm-9">
              <%= documents_form.text_field :cpf, class: "form-control cpf-mask" %>
            </div>
          </div>
        </div>
      </div>
      <div class="row panel-body">
        <%= documents_form.fields_for :identity_document do |identity_document_form| %>
          <%= render 'identity_documents/form_identity_document', identity_document_form:identity_document_form %>
        <% end %>
      </div>
      <div class="row panel-body">
        <%= documents_form.fields_for :certificate do |certificate_form| %>
          <%= render 'certificates/form_certificate', certificate_form:certificate_form %>
        <% end %>
      </div>
    </div>
  </div>
</div>