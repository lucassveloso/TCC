<div class="text-center">
  <h1>Cadastrar Aluno</h1>
</div>
<%= form_for(@student, :html=>{:id=>"form-student"}) do |f| %>
<%= render 'students/partials/form/form', f:f %>
<div class="row">
  <div class="form-group">
    <div class="col-lg-offset-4 col-lg-4 margin-end-top margin-end-bot">
      <%= f.submit "Criar Aluno", :class => "btn btn-default btn-lg btn-block btn-primary", id: "confirm-student" %>
    </div>
  </div>
</div>
<% end %>